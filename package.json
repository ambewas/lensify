{
  "name": "lensify",
  "version": "0.1.1",
  "description": "Takes a (state) object and returns identical structure, where every key is a (ramda) lens",
  "main": "dist/index.js",
  "keywords": [
    "functional",
    "lens"
  ],
  "author": "kevin decock",
  "license": "ISC",
  "scripts": {
    "watch:js": "onchange \"src/\" -- npm run bundle",
    "clear": "rimraf dist/**",
    "lint": "esw src",
    "lint:watch": "esw src -w",
    "test": "jest --env=jsdom --watch",
    "test:watch": "npm run test -- -w",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage:serve": "lite-server",
    "rollup": "rollup --config rollup.config.js",
    "build": "npm run rollup",
    "bundle": "npm-run-all -s lint build",
    "prepublish": "npm-run-all -s clear bundle"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "copy": "^0.3.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.19.0",
    "eslint-watch": "^3.1.0",
    "jest": "^20.0.3",
    "lite-server": "^2.3.0",
    "npm-run-all": "^4.0.2",
    "npm-watch": "^0.1.9",
    "nyc": "^10.3.2",
    "onchange": "^3.2.1",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-jsx": "^1.0.3",
    "rollup-plugin-uglify-es": "^0.0.1",
    "uglify-es": "^3.0.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ambewas/lensify"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "dependencies": {
    "ramda": "^0.23.0"
  }
}
